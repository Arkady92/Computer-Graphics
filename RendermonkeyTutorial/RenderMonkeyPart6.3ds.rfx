    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D src;
float luminance_cutoff;

float4 ps_main( float2 Tex : TEXCOORD0 ) : COLOR0
{
   float4 color = tex2D(src,Tex);
   return dot(color.xyz,float3(1.0,1.0,1.0)) < luminance_cutoff ? float4(.0,.0,.0,.0) : color;
}



