float4x4 matViewProjection;
float4x4 matViewInverse;
float time;
float xmax;
float vmax;
float thalf;

struct VS_INPUT 
{
   float4 Position : POSITION0;
   float2 Texcoord : TEXCOORD0;
   float3 Normal :   NORMAL0;
   float3 Tangent : TANGENT0;
   float3 Binormal : BINORMAL0;
};

struct VS_OUTPUT 
{
   float4 Position :        POSITION0;
   float2 Texcoord :        TEXCOORD0;
   float3 ViewDirection :   TEXCOORD1;
   float3 Normal :          TEXCOORD2;
   float3 Tangent :         TEXCOORD3;
};

float spring_solution(float t)
{
   return xmax * exp(-0.5*(1.38629/thalf)*t)*sin(vmax/xmax*t);
}

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output;

   Output.Position = mul( Input.Position + float4(.0,30.0+spring_solution(time),.0,.0), matViewProjection );
   Output.Texcoord         = Input.Texcoord * 0.4;
   
   float3 fvObjectPosition = Input.Position;
   Output.ViewDirection    = mul( float4(0,0,0,1), matViewInverse ) -
   fvObjectPosition;
   Output.Normal = Input.Normal;
   Output.Tangent = Input.Tangent;
      
   return( Output );
   
}


]]></RmShaderCode>
