   float2 TexCoord : TEXCOORD0;   
};

VS_OUTPUT vs_main( VS_INPUT In )
{
   VS_OUTPUT Out;
   Out.Position.xy = sign(In.Position);
   Out.Position.z = 1.0;
   Out.Position.w = 1.0;
   Out.TexCoord.x = Out.Position.x * 0.5 + 0.5;
   Out.TexCoord.y = 1.0 - (Out.Position.y * 0.5 + 0.5);
   return Out;
}



